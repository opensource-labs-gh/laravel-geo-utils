{
    "name": "opensource-labs-gh/laravel-geo-utils",
    "description": "A Laravel utility package for geographic polygon point checks with support for both PHP algorithms and MySQL spatial functions.",
    "type": "library",
    "license": "MIT",
    "authors": [
      {
        "name": "Mawuli Agbenyo",
        "email": "mawulikofiagbenyo@gmail.com",
        "homepage": "https://github.com/opensource-labs-gh"
      }
    ],
    "autoload": {
      "psr-4": {
        "OpensourceLabsGh\\GeoUtils\\": "src/"
      }
    },
    "autoload-dev": {
      "psr-4": {
        "OpensourceLabsGh\\GeoUtils\\Tests\\": "tests/"
      }
    },
    "require": {
      "php": ">=8.1",
      "illuminate/support": "^9.0|^10.0|^11.0|^12.0",
      "illuminate/database": "^9.0|^10.0|^11.0|^12.0"
    },
    "require-dev": {
      "phpunit/phpunit": "^10.0",
      "orchestra/testbench": "^8.0|^9.0|^10.0",
      "mockery/mockery": "^1.4"
    },
    "extra": {
      "laravel": {
        "providers": [
          "OpensourceLabsGh\\GeoUtils\\GeoServiceProvider"
        ],
        "aliases": {
          "GeoHelper": "OpensourceLabsGh\\GeoUtils\\Facades\\GeoHelper"
        }
      }
    },
    "scripts": {
      "test": "vendor/bin/phpunit"
    },
    "keywords": [
      "laravel",
      "geo",
      "polygon",
      "utilities",
      "gis",
      "coordinates",
      "spatial",
      "geometry",
      "point-in-polygon"
    ],
    "minimum-stability": "dev",
    "prefer-stable": true
  }